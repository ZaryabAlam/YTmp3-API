{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "source": "/download",
      "methods": [
        "POST"
      ],
      "destination": "app.py"
    },
    {
      "source": "/downloads/(.*)",
      "methods": [
        "GET"
      ],
      "destination": "app.py"
    }
  ]
}